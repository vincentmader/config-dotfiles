{
  // Setup Diagnostic Symbols & Indicators.
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.warningSign": "⚠",
  "diagnostic.hintSign": "➤",
  "suggest.snippetIndicator": " ►",

  // ...
  "coc.preferences.messageLevel": "error",

  // Setup Filetype-Specific Auto-Linting.
  "coc.preferences.formatOnSave": true,
  // "coc.preferences.formatOnSaveFiletypes": [
  //   "dart",
  //   "javascript",
  //   "javascriptreact",
  //   "json",
  //   "markdown",
  //   "python",
  //   "rust",
  //   "toml",
  //   "typescriptreact"
  // ],
  // "css",
  // "typescript",

  // Setup Snippet Directory(ies).
  "snippets.ultisnips.directories": ["UltiSnips", "snippets"],
  // Setup Snippet Filetype Extensions.
  "snippets.extends": {
    "cpp": ["c"],
    "javascriptreact": ["javascript"],
    "tera": ["html"]
  },
  // "typescript": ["javascript"],

  // Setup Python Linting.
  "python.linting.enabled": true,
  "python.linting.banditEnabled": false,
  "python.linting.flake8Enabled": false,
  "python.linting.mypyEnabled": false,
  "python.linting.prospectorEnabled": false,
  "python.linting.pycodestyleEnabled": false,
  "python.linting.pydocstyleEnabled": false,
  "python.linting.pylamaEnabled": false,
  "python.linting.pylintEnabled": true,
  "python.linting.pylintPath": "/opt/homebrew/bin/pylint",
  "python.linting.pytypeEnabled": false,
  "python.linting.pylintArgs": ["--disable=C0103", "--disable=C0116"],
  "python.venvPath": "",
  "python.pythonPath": "/opt/homebrew/bin/python3",
  "pyright.inlayHints.functionReturnTypes": false,
  "pyright.inlayHints.variableTypes": false,

  // Setup Rust Linting.
  "rust-analyzer.enable": true,
  "rust-analyzer.server.path": "/opt/homebrew/bin/rust-analyzer",
  "rust-analyzer.inlayHints.parameterHints.enable": true,
  "rust-analyzer.inlayHints.typeHints.enable": true,
  "rust-analyzer.inlayHints.chainingHints.enable": true,
  "rust-analyzer.checkOnSave.enable": true,
  "rust-analyzer.diagnostics.enable": false,

  // Setup JS Linting.
  "eslint.options": {
    "configFile": "/home/vinc/.config/eslint/eslintrc"
  }

